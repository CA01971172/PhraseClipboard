{
    "name": "Phrase Clipboard",
    "description": "保存した定型文をクリップボードにコピー",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "image/icon16.png",
        "48": "image/icon48.png",
        "128": "image/icon128.png"
    },  
    "permissions": [ 
        "storage"
    ],
    "action": {
        "default_icon": "image/icon128.png",
        "default_title": "Phrase Clipboard",
        "default_popup": "popup.html"
    }
}